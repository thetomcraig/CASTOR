{
  "name": "notes",
  "version": "1.0.0",
  "description": "Personal, private, notes",
  "author": "Tom Craig",
  "license": "ISC",
  "remarkConfig": {
    "settings": {
      "bullet": "*",
      "listItemIndent": 1
    },
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-list-item-indent",
        "space"
      ],
      [
        "lint-final-newline",
        false
      ]
    ]
  },
  "dependencies": {
    "remark": "^11.0.1",
    "remark-cli": "^7.0.0",
    "remark-lint": "^6.0.5",
    "remark-preset-lint-recommended": "^3.0.3"
  }
}
